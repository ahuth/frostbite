{
  "name": "frostbite",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://ahuth.github.io/frostbite",
  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build --git $(which git)",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "predeploy": "yarn run build",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "gh-pages": "^2.1.1",
    "typescript": "^3.7.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
